{
  "name": "@sezercakir/ui",
  "version": "2.0.0",
  "author": "Talha Sezer Cakir",
  "description": "A personalised UI Library.",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c --watch",
    "storybook": "storybook dev -p 6006",
    "removeDist": "rm -rf dist",
    "build-storybook": "storybook build",
    "build": "npm run removeDist && npm run rollup && npm run post:build",
    "buildAndPublish": "npm run build && npm run publish",
    "publish": "npm publish ./dist",
    "post:build": "node ./scripts/postBuild.js",
    "rollup": "rollup -c"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.4",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/react-webpack5": "^7.4.5",
    "@storybook/test": "^7.6.10",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.23",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.68.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "storybook": "^7.4.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
